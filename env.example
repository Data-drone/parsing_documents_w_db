# Sample .env file for Databricks environment

# Your Databricks personal access token
DATABRICKS_TOKEN='your-token-here'

# Your cluster ID
DATABRICKS_CLUSTER_ID='your-cluster-id-here'

# Your Databricks CLI profile
DATABRICKS_PROFILE='your-profile-here'

# Workspace root path (e.g., /Workspace/Users/your.email@databricks.com/project)
DATABRICKS_WORKSPACE_ROOT='/Workspace/Users/your.email@databricks.com/parsing_documents_w_db' 

# Unity Catalog configuration (leave blank to use notebook defaults)
CATALOG_NAME='my_catalog'
SCHEMA_NAME='tutorials'
VOLUME_NAME='sample_docs' 

# GenAI Services 
VECTOR_SEARCH_ENDPOINT='one-env-shared-endpoint-2'
LLM_MODEL='databricks-claude-3-7-sonnet'
EMBEDDING_MODEL='databricks-gte-large-en'

# Use serverless compute for tests (true/false)
USE_SERVERLESS='false' 